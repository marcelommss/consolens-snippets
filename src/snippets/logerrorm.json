{
  "Consolens Log Error with hard code source map": {
    "prefix": "logerror",
    "body": [
      "$1logError({",
      "\tmessage: '$2',",
      "\targs: [$3error],",
      "\tisEffect: $4,",
      "\tfunctionName: '$5',",
      "\tsource: '${TM_DIRECTORY}/{$TM_FILENAME}',",
      "\tline: `${TM_CURRENT_LINE}`,",
      "});",
      "$0"
    ],
    "description": "Log an error using Consolens"
  }
}
