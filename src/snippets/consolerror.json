{
  "Consolens Log Error (complete)": {
    "prefix": "consolerror",
    "body": [
      "$1logError({",
      "\tmessage: '$2',",
      "\targs: [${3:error}],",
      "\tmessageColor: '$4',",
      "\tcontext: '$5',",
      "\ttags: [$6],",
      "\tgroup: '$7',",
      "\tparentGroup: '$8',",
      "\tgroupColor: $9",
      "\tfunctionName: '$10',",
      "\tisEffect: $11,",
      "\tsource: '${TM_DIRECTORY}/{$TM_FILENAME}',",
      "\tline: `${TM_CURRENT_LINE}`,",
      "});",
      "$0"
    ],
    "description": "Log an error message using Consolens with all LogParams properties"
  }
}
