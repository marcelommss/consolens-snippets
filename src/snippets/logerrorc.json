{
  "Consolens Log Error": {
    "prefix": "logerror",
    "body": [
      "$1logError({",
      "\tmessage: '$2',",
      "\targs: [$3error],",
      "\tcontext: '$4',",
      "\ttags: [$5],",
      "\tgroup: '$6',",
      "\tparentGroup: '$7',",
      "});",
      "$0"
    ],
    "description": "Log an error using Consolens"
  }
}
