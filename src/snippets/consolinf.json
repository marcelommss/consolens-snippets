{
  "Consolens Log Information (complete)": {
    "prefix": "consolinf",
    "body": [
      "logInfo({",
      "  functionName: '$1',",
      "  isEffect: $2,",
      "  description: '$3',",
      "  args: [$4],",
      "  messageColor: '$5',",
      "  context: '$6',",
      "  tags: [$7],",
      "  group: '$8',",
      "  parentGroup: '$9',",
      "  groupColor: $10",
      "  source: '${TM_DIRECTORY}/{$TM_FILENAME}',",
      "  line: `${TM_CURRENT_LINE}`,",
      "});",
      "$0"
    ],
    "description": "Log an informational message using Consolens with all LogParams properties"
  }
}
