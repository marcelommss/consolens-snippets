{
  "Consolens Log Information (complete)": {
    "prefix": "consolinf",
    "body": [
      "logInfo$1({",
      "\tmessage: '$2',",
      "\tmessageColor: '$3',",
      "\targs: [$4],",
      "\tcontext: '$5',",
      "\ttags: [$6],",
      "\tgroup: '$7',",
      "\tparentGroup: '$8',",
      "\tgroupColor: $9",
      "\tfunctionName: '$10',",
      "\tisEffect: $11,",
      "\tsource: '${TM_DIRECTORY}/{$TM_FILENAME}',",
      "\tline: `${TM_CURRENT_LINE}`,",
      "});",
      "$0"
    ],
    "description": "Log an informational message using Consolens with all LogParams properties"
  }
}
